{
	"name": "starter-next",
	"version": "1.0.2",
	"keywords": [
		"react",
		"next",
		"starter"
	],
	"private": true,
	"scripts": {
		"/": "-- NEXT --",
		"dev": "next dev",
		"build": "next build",
		"start": "next start -p 8080",
		"//": "-- CKECKS --",
		"prettier": "prettier --write .",
		"lint": "next lint",
		"lint::scss": "stylelint \"**/*.scss\"",
		"///": "-- DOCKER --",
		"docker-dev": "docker-compose -f build/dev.docker-compose.yml up --build --force-recreate",
		"docker-prod": "docker-compose -f build/prod.docker-compose.yml up --build --force-recreate",
		"////": "-- POSTINSTALL --",
		"postinstall": "husky install"
	},
	"dependencies": {
		"@types/node": "18.11.11",
		"@types/react": "18.0.26",
		"@types/react-dom": "18.0.9",
		"next": "13.0.6",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@svgr/webpack": "^6.5.1",
		"@types/css-modules": "^1.0.2",
		"eslint": "8.29.0",
		"eslint-config-next": "13.0.6",
		"eslint-config-prettier": "^8.5.0",
		"husky": "^8.0.2",
		"prettier": "^2.8.1",
		"sass": "^1.56.1",
		"stylelint": "^14.16.0",
		"stylelint-config-css-modules": "^4.1.0",
		"stylelint-config-prettier-scss": "^0.0.1",
		"stylelint-config-property-sort-order-smacss": "^9.0.0",
		"stylelint-config-standard-scss": "^6.1.0",
		"typescript": "4.9.4"
	}
}
